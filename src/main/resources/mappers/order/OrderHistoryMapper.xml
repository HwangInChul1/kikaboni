<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kikaboni.project.repository.OrderHistoryRepository">

	<select id="orderList" resultType="OrderHistoryVO">
		select * from order_history 
	</select>

	<select id="getList" resultType="OrderHistoryVO">
		select * from order_history where memberId = #{memberId}
	</select>
	

</mapper>